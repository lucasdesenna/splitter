[ignore]
.*/node_modules/.*

[include]
./src/

[libs]
./flow-typed

[options]
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/config/CSSModule.flow.js'
module.name_mapper.extension='sass' -> '<PROJECT_ROOT>/config/CSSModule.flow.js'
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
unsafe.enable_getters_and_setters=true
